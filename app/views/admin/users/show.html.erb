<%= link_to "All Users", admin_users_path, class: "new_edit" %>
<hr />

<h3>
  <%= @admin_user.full_name %>
</h3>
<h4>
  <%= @admin_user.email %>
</h4>
<p> 
  Created at <%= format_date(@admin_user.created_at) %> <br />
  Last updated at <%= format_date(@admin_user.updated_at) %>
</p>
<hr />

<%= link_to "Edit user", edit_admin_user_path(@admin_user), class: "new_edit" %> <br />
<%= link_to "Delete user", admin_user_path(@admin_user), method: :delete, 
    data: { confirm: "Are you sure you want to delete this user?" }, 
    class: "new_edit" %>